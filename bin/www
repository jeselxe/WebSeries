#!/usr/bin/env node

var app = require('../app');
var models = require("../models");
var data = require('../data');

var PORT = process.env.PORT || 8000;
if (process.env.PROD) {
	app.listen(PORT, function () {
			console.log("Server running on port " + PORT);
	});
} else {
	data.fillData()
	.then(function(){
		app.listen(PORT, function () {
			console.log("Server running on port " + PORT);
		});
	});
}